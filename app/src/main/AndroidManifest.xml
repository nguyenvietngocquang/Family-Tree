<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.familytree">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".notification.NotificationActivity"></activity>
        <activity android:name=".random_member.RandomMemberActivity" />
        <activity android:name=".list_member.EditMemberActivity" />
        <activity android:name=".view_tree.ChooseMemberActivity" />
        <activity android:name=".about.AboutActivity" />
        <activity android:name=".list_member.ListMemberActivity" />
        <activity android:name=".add_member.AddMemberActivity" />
        <activity android:name=".view_tree.ViewTreeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>